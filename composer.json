{
    "name": "codewithkyle/read-only",
    "description": "Adds a read only text field to Craft CMS with click-to-copy functionality.",
    "type": "craft-plugin",
    "version": "1.0.5",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "read only"
    ],
    "support": {
        "docs": "https://github.com/codewithkyle/craft-readonly/blob/master/README.md",
        "issues": "https://github.com/codewithkyle/craft-readonly/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kyle Andrews",
            "homepage": "https://kyleandrews.dev/"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0",
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
          "codewithkyle\\readonly\\": "src/"
        }
    },
    "extra": {
        "name": "Read Only",
        "handle": "read-only",
        "developer": "Kyle Andrews",
        "developerUrl": "https://kyleandrews.dev/",
        "documentationUrl": "https://github.com/codewithkyle/craft-readonly/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/codewithkyle/craft-readonly/master/CHANGELOG.md",
        "class": "codewithkyle\\readonly\\ReadOnlyPlugin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
